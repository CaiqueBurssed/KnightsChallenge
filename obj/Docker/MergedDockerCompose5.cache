name: dockercompose1688047108739972838
services:
  knightsapi:
    build:
      context: C:\Users\user\Desktop\KnightsChallenge
      dockerfile: KnightsAPI/Dockerfile
    container_name: KnightsAPI
    depends_on:
      knightschallengedb:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DatabaseSettings:ConnectionString: mongodb://knightschallengedb:27017
    image: knightsapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        published: "8000"
        protocol: tcp
  knightschallengedb:
    container_name: knightschallengedb
    image: mongo
    networks:
      default: null
    ports:
      - mode: ingress
        target: 27017
        published: "27017"
        protocol: tcp
    restart: always
    volumes:
      - type: volume
        source: mongo_data
        target: /data/db
        volume: {}
networks:
  default:
    name: dockercompose1688047108739972838_default
volumes:
  mongo_data:
    name: dockercompose1688047108739972838_mongo_data